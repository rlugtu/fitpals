<%- include('./partials/boilerplate') %>

<div class=scheduleContent>
    <div class="fullSchedule">
        <h1 class="scheduleHeader">Completed</h1>
        <div class="fullScheduleContent">
            <% workouts.forEach(workout => {  %>
            <%if(workout.status) { %>
            <div class="singleWorkouts">
                <div class="date">
                    <h3><a href='/schedule/<%= workout.id%>/'><%= moment(workout.date).format("MMM Do YYYY");  %></h3>
                </div>
                <div class="split">
                    <h3><%= workout.split %></h3>
                </div>
                <div class="status"><% if (workout.status) { %>
                    Completed
                    <% } else { %>
                    Incomplete
                    <% } %>
                </div>
            </div>
            <% }}) %>
        </div>
    </div>
</div>

<%- include('./partials/end') %>